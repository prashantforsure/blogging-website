
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id     Int @id @default(autoincrement())
  name String
  username String @unique
  password String
  blogs Blog[]
 
}

model Blog {
   id     Int @id @default(autoincrement())
   title String
      authorId Int 
   content String
    thumbnail String?
    published Boolean @default(false)
    author User @relation(fields: [authorId], references: [id])
}